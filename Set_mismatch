class Solution {
    public int[] findErrorNums(int[] nums) {
       int n = nums.length;
       int[] ansarr = new int[n+1];
       int rep = 0;
       int dup = 0;
       for(int k : nums){
        ansarr[k]++;
       }
       for(int i = 1 ;  i <= n ; i++){
        if(ansarr[i] == 2){
            dup = i;
        }
        if(ansarr[i] == 0){
            rep = i;
        }
       }
    return new int[]{dup,rep};
    }
}
